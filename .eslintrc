{
    "rules": {
        "indent": 0,
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "comma-dangle": [
            2,
            "always-multiline"
        ],
    },
    "env": {
        "browser": true
    },
    "globals": {
        " (library names) ": true,
        "$": true,
        "JSZip": true,
        "Mustache": true,
        "ExcelBuilder": true,
        "saveAs": true,
        "XLSX": true,
    },
    "extends": "eslint:recommended"
}